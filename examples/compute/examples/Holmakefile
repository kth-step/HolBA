CLINE_OPTIONS = -r

INCLUDES = $(SMOLBIRDIR)/examples/increment \
					 $(SMOLBIRDIR)/examples/and_not \
  				 $(SMOLBIRDIR)/examples/mem_incr \
					 $(SMOLBIRDIR)/examples/sum_list \
					 $(SMOLBIRDIR)/examples/jump_chain

all: $(DEFAULT_TARGETS) test-examples.exe
.PHONY: all

